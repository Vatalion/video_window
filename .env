# Environment configuration for Video Window
# Copy this file to .env.local and update with your values

# API Configuration
API_BASE_URL=https://api.video_window.local
API_TIMEOUT=30000

# Authentication
ENABLE_BIOMETRIC_AUTH=true
SESSION_TIMEOUT=3600

# Debug Settings
DEBUG_MODE=true
ENABLE_LOGGING=true
LOG_LEVEL=debug

# Development Settings
SKIP_SSL_VERIFICATION=true
MOCK_BACKEND=false
